{
	"builds": [
		{
			"src": "public/**",
			"use": "@now/static"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"headers": {
				"Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload"
			},
			"continue": true
		},
		{
			"src": "/x(.*)",
			"dest": "https://budui.github.io/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/public/$1"
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"status": 404,
			"dest": "/public/404.html"
		}
	]
}